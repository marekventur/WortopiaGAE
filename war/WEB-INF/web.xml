<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>WortopiaGAE</servlet-name>
		<servlet-class>de.wortopia.WortopiaGAEServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>InitialLoading</servlet-name>
		<servlet-class>de.wortopia.InitialLoading</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ImportUser</servlet-name>
		<servlet-class>de.wortopia.ImportUser</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>Recache</servlet-name>
		<servlet-class>de.wortopia.Recache</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>Index</servlet-name>
		<servlet-class>de.wortopia.controller.Index</servlet-class>
	</servlet>	
	
	<servlet-mapping>
		<servlet-name>WortopiaGAE</servlet-name>
		<url-pattern>/wortopiagae</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ImportUser</servlet-name>
		<url-pattern>/importUser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>InitialLoading</servlet-name>
		<url-pattern>/initialLoading</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Recache</servlet-name>
		<url-pattern>/recache</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index</welcome-file>
	</welcome-file-list>
</web-app>
